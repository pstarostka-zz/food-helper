{
  "name": "food-helper",
  "private": true,
  "sideEffects": false,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --stage",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "nx affected:lint & nx affected:test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@angular/animations": "^10.0.3",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "^10.0.3",
    "@angular/compiler": "^10.0.3",
    "@angular/core": "^10.0.3",
    "@angular/forms": "^10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "^10.0.3",
    "@angular/platform-browser-dynamic": "^10.0.3",
    "@angular/router": "^10.0.3",
    "@nrwl/angular": "10.0.0-beta.5",
    "rxjs": "^6.6.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.2",
    "@angular/cli": "10.0.2",
    "@angular/compiler-cli": "^10.0.3",
    "@angular/language-service": "^10.0.3",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@nrwl/cypress": "10.0.0-beta.5",
    "@nrwl/jest": "10.0.0-beta.5",
    "@nrwl/next": "^9.5.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "10.0.0-beta.5",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "codelyzer": "^6.0.0",
    "commitlint": "^9.0.1",
    "cypress": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "standard-version": "^8.0.1",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "~3.9.6"
  }
}
